			  var hideRight = $(window).width() - offsetLeft;
			  if ( direction == 'left' ) {
				  currPos = hideLeft;
				  nextPos = hideRight;
			  }
			  else {
				  currPos = hideRight;
				  nextPos = hideLeft;
			  }
